msg.topic="cambio_all";

//Dolar_Canadense
global.set("cad",msg.payload.CAD.ask);
global.set("cadVarBid",msg.payload.CAD.varBid);
global.set("cadTime",msg.payload.CAD.create_date);

//Euro
global.set("eur",msg.payload.EUR.ask);
global.set("eurVarBid",msg.payload.EUR.varBid);
global.set("eurTime",msg.payload.EUR.create_date);

//Libra
global.set("gbp",msg.payload.GBP.ask);
global.set("gbpVarBid",msg.payload.GBP.varBid);
global.set("gbpTime",msg.payload.GBP.create_date);

//Peso Argentino
global.set("ars",msg.payload.ARS.ask);
global.set("arsVarBid",msg.payload.GBP.varBid);
global.set("arsTime",msg.payload.GBP.create_date);

//Bitcoin
global.set("btc",msg.payload.BTC.ask);
global.set("btcVarBid",msg.payload.BTC.varBid);
global.set("btcTime",msg.payload.BTC.create_date);